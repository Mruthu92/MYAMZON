pipeline {
agent any
    environment {
		mavenHome = tool 'mavennew'
	}
  stages{
       stage('GetCode'){
            steps{
                git branch: 'master',
                url: 'https://github.com/Mruthu92/MYAMZON.git'
                sh "ls -lat"             
            }
         }   
  
		stage('Build') {
			steps {
				sh 'mvn -DskipTests clean install'
			}
		}
		stage('Test') {
			steps {
				sh 'mvn test'
			}
			post {
				always {
					junit '**/target/surefire-reports/*.xml'
				}
			}
		}
		stage('Deliver') {
			steps {
				sh './scripts/deliver.sh'
			}
		}
	

 }
}
